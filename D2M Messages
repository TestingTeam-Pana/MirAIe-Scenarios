    D2M_MSG_REGISTRATION_CANT_RCH_SERVER        = 104, // Any other error

 

    // IAM Login

 

    D2M_MSG_IAM_LOGIN_SUCCESS                   = 112, // (TBD)

 

    D2M_MSG_IAM_AUTHENTICATION_ERROR            = 118, // HTTP 401

 

    D2M_MSG_IAM_SERVER_INTERNAL_ERROR           = 122, // HTTP 500

 

    D2M_MSG_IAM_RESPONSE_PARSING_SERVER         = 121, // Response parsing error

 

